{
  "expo": {
    "name": "getwish",
    "slug": "getwish",
    "version": "1.3",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/images/auth/background.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 3000,
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.wish_list",
      "versionCode": 6,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "ios": {
      "bundleIdentifier": "com.getwish",
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires access to the camera to take photos to upload your profile photo.",
        "NSPhotoLibraryUsageDescription": "This app requires access to the photo library to upload your photo to the your posts."
      },
      "buildNumber": "1.1"
    },
    "extra": {
      "eas": {
        "projectId": "f5879f84-7eff-49bc-bd6e-0f396a1c34b0"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
